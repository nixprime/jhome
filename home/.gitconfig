[user]
  name = Jamie Liu
  email = jliu@nixprime.com
[sendemail]
  from = Jamie Liu <jliu@nixprime.com>
[core]

[alias]
  br = branch
  changes = diff --name-status -r
  ci = commit
  co = checkout
  history = reflog --date=relative
  ignored = ls-files --others -i --exclude-standard
  ignored-paths = clean -ndX
  unstage = reset HEAD
  st = status
[branch]
  autosetupmerge = always
[color]
  ui = true
[diff]
  algorithm = patience
[push]
  default = simple
